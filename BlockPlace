wait(2)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local BlockPlace = OrionLib:MakeWindow({Name = "Block Place | DarkStarsGaming#6722", HidePremium = false, IntroEnabled = true, IntroText = "Dark's Script", SaveConfig = true, ConfigFolder = "BlockPlace"})
_G.Player1 = "string"
_G.Player2 = "string"
_G.Coordinates = "string"
_G.autoAction = true

function autoAction()
    while _G.autoAction == true do
        wait(62)
        local args = {
            [1] = ">getactions",
            [2] = "All"
        }

        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))  
    end
end

-- Admin Commands
local AdminCommandsTab = BlockPlace:MakeTab({
    Name = "Self Commands",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

AdminCommandsTab:AddButton({
    Name = "Give Actions",
    Callback = function()
        local args = {
            [1] = ">getactions",
            [2] = "All"
        }

        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
    end    
})

AdminCommandsTab:AddToggle({
    Name = "Auto Actions!",
    Default = false,
    Callback = function(Value)
        print(Value)
        _G.autoAction = Value
        autoAction()
    end    
})

AdminCommandsTab:AddButton({
    Name = "Hide Yourself",
    Callback = function()
        local args = {
            [1] = ">hide DarkStarzGaming",
            [2] = "All"
        }

        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
    end    
})

AdminCommandsTab:AddButton({
    Name = "Show Command List",
    Callback = function()
        local args = {
            [1] = ">cmds",
            [2] = "All"
        }

        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))   
    end    
})

AdminCommandsTab:AddTextbox({
    Name = "Crash",
    Default = "",
    TextDisappear = true,
    Callback = function(txt)
        local args = {
            [1] = ">crash "..txt.."",
            [2] = "All"
